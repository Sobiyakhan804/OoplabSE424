#include <iostream>
using namespace std;

class Car {
private:
    string name;
    float topSpeed;
    int modelName;

public:
    
    Car(string Name, float TopSpeed, int ModelName) {
        name = Name;
        topSpeed = TopSpeed;
        modelName = ModelName;
    }

    
    void display() {
        cout << "the name of car is" << name << endl;
        cout << "speed of the car is " << topSpeed  << endl;
        cout << "model of the car is " << modelName << endl;
    }
};

int main() {
   
    Car c("Ferrari", 459.00, 2025);

    
    c.display();

    return 0;
}
